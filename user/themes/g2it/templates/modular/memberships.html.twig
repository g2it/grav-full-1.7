<div class="memberships">
  <div class="container">
    <div class="row">
      <div class="col-md-24">
        <h1 class="section-header">{{ page.header.title }}</h1>
      </div>
    </div>
    <div class="row">
      <div class="col-12 owl-carousel text-center">
        {% for member in page.header.members %}
        <div class="item">
          <a href="{{ member.url }}">
            <img src="{{ page.media.images[member.logo].cache.url() }}" alt="{{ member.name|e('html') }}">
          </a>
        </div>
        {% endfor %}
      </div>
    </div>
  </div>
</div>
