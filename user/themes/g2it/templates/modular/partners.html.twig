<div class="our-partners-section partners">
  <div class="container">
    <div class="row">
      <div class="col-md-24">
        <h1 class="section-header">{{ page.header.title }}</h1>
      </div>
    </div>
    <div class="row">
      <div class="col-md-24">
        <div class="our-partners-slider">
          <div class="our-partners-slider__partners">
            <ul class="owl-carousel">
              {% set collection = page.evaluate([{'@page.children':'/our-partners'}]) %}
              {% set ordered_collection = collection.order('default','asc') %}
              {% for partner_p in ordered_collection%}
                <li class="item">
                  <img src="{{ partner_p.media.images|first.cropResize(200,70).url() }}" alt="{{ partner_p.title|e('html') }}">
                </li>
              {% endfor %}
            </ul>
          </div>

        </div>
      </div>
    </div>
  </div>
</div>
